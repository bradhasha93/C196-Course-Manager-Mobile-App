<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wgu.com.bhasha.c196scheduler">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MentorsOverviewActivity" />
        <activity android:name=".MentorsDetailActivity" />
        <activity android:name=".AssessmentsOverviewActivity" />
        <activity android:name=".AssessmentsDetailActivity" />
        <activity android:name=".CoursesOverviewActivity" />
        <activity android:name=".CoursesDetailActivity" />
        <activity android:name=".TermsOverviewActivity" />
        <activity android:name=".TermsDetailActivity" />

        <receiver
            android:name=".receivers.NotificationReceiver"
            android:enabled="true"
            android:exported="true"></receiver>
    </application>

</manifest>